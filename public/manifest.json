{
  "manifest_version": 3,
  "name": "TooManyTabs",
  "version": "1.0.0",
  "description": "Save all open tabs into a list, just like OneTab",
  "permissions": [
    "tabs",
    "storage",
    "contextMenus"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.svg",
    "48": "icon.svg",
    "128": "icon.svg"
  }
}
  